{
  "expo": {
    "name": "ledgerly-mobile",
    "slug": "ledgerly-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-square.png",
    "scheme": "ledgerlymobile",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon-square.png"
      },
      "package": "com.mekhanofficial.ledgerlymobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ledgerly-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ledgerly-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1c956fc1-54f8-424a-aae7-b8e1cc89e3aa"
      }
    }
  }
}
